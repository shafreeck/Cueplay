{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-set-fullscreen",
    "core:window:allow-set-fullscreen",
    "core:window:allow-is-fullscreen",
    "core:window:allow-start-dragging",
    "os:default",
    "shell:allow-open",
    "process:allow-restart",
    "updater:default",
    "dialog:default"
  ]
}